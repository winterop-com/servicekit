# Service Registration Configuration
# Copy this file to .env and adjust values as needed

# Orchestrator URL (where to register this service)
SERVICEKIT_ORCHESTRATOR_URL=http://orchestrator:9000/register

# Service hostname (auto-detected from Docker container if not set)
# SERVICEKIT_HOST=my-service

# Service port (defaults to 8000 if not set)
# SERVICEKIT_PORT=8000

# Logging
LOG_FORMAT=json
LOG_LEVEL=INFO

# Custom environment variable names (optional - override defaults)
# MY_ORCHESTRATOR_URL=http://my-orchestrator:9000/register
# MY_SERVICE_HOST=my-service
# MY_SERVICE_PORT=8000
